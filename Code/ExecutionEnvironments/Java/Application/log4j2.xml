<Configuration status="debug" name="baeldung" packages="">
    <Appenders>
        <Console name="stdout" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n"/>
        </Console>
    </Appenders>
</Configuration>
<Root level="error">
    <AppenderRef ref="stdout"/>
</Root>
<Root level="INFO">
    <AppenderRef ref="stdout" />
</Root>
<Root level="TRACE">
    <AppenderRef ref="stdout" />
</Root>
<Root level="DEBUG">
    <AppenderRef ref="stdout" />
</Root>